<script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '343395636045684',
      xfbml      : true,
      version    : 'v2.8'
    });
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "//connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
</script>

<div class="container">
  <br>
  <h2>Welcome to RudynB</h2>


  <% if signed_in? %>
    <%= image_tag current_user.profile_pic.thumb.url, class: "img-circle" %>

    <p>
      Hi <%= current_user.firstname %>!
    </p>

    <%= current_user.email %>
    <br><br>

    <p><%= link_to "User Profile", user_path(current_user) %></p>


    <p><%= link_to "Home", home_path %></p>

    <p><%= link_to 'See Listings', '/listings' %></p>

    <p><%= link_to 'My reservation', [current_user, :reservation] %></p>

  <% else %>

  <% end %>




  <div
    class="fb-like"
    data-share="true"
    data-width="450"
    data-show-faces="true">
  </div>



</div>
